{
  "name": "kirillvasiltsov.com",
  "description": "Kirill Vasiltsov's personal website",
  "version": "0.0.1",
  "scripts": {
    "dev": "npm run tailwind && sapper dev",
    "tailwind": "tailwindcss build tailwind.css -o static/tailwind.css -c tailwind.config.js",
    "build": "cross-env NODE_ENV=production npm run tailwind && sapper build",
    "export": "cross-env NODE_ENV=production npm run tailwind && sapper export --timeout 90000",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "dotenv": "^8.2.0",
    "glob": "^7.1.6",
    "gray-matter": "^4.0.2",
    "jimp": "^0.13.0",
    "marked": "^1.1.0",
    "node-fetch": "^2.6.0",
    "polka": "next",
    "query-string": "^6.12.1",
    "shiki": "^0.1.7",
    "sirv": "^0.4.0",
    "tailwindcss": "^1.4.6",
    "tmp": "^0.2.1",
    "wasm-twitter-card": "^0.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "cross-env": "^7.0.2",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^5.3.0",
    "sapper": "^0.27.14",
    "svelte": "^3.0.0"
  }
}
